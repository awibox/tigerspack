{
  "name": "tigerspack",
  "version": "0.0.5",
  "description": "UI Framework",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "cd website && yarn start",
    "lint": "eslint --report-unused-disable-directives src",
    "fix": "eslint --fix --report-unused-disable-directives src",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "setup": "cp ./.github/hooks/pre-commit ./.git/hooks/pre-commit && chmod +x ./.git/hooks/pre-commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tigerspack/tigerspack.git"
  },
  "author": "awibox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tigerspack/tigerspack/issues"
  },
  "homepage": "https://github.com/tigerspack/tigerspack#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^25.1.0",
    "jest-aphrodite-react": "^2.2.0",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.2.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "aphrodite": "^2.4.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/**/index.{js,jsx,mjs}",
      "!src/index.js"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "snapshotResolver": "<rootDir>/spec/jestSnapshotResolver",
    "snapshotSerializers": [
      "jest-aphrodite-react"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/spec/jestSetupTests.js",
      "<rootDir>/spec/jestSetupJsDom.js"
    ]
  }
}
